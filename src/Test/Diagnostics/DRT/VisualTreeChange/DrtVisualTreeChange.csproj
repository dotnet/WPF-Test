<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>DrtVisualTreeChange</AssemblyName>
    <AssemblyIdentityVersionName>WCP</AssemblyIdentityVersionName>
    <OutputType>EXE</OutputType>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <LinkSubsystem>console</LinkSubsystem>
    <TestCode>true</TestCode>
  </PropertyGroup>
  <PropertyGroup>
    <StartupObject />
  </PropertyGroup>
  <PropertyGroup>
    <RootNamespace>DrtVisualTreeChange</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug|AnyCPU'">
    <UseVSHostingProcess>false</UseVSHostingProcess>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="$(DrtCommonRoot)\inc\DrtBase.cs" />
    <Compile Include="DrtVisualTreeChange.cs" />
    <Compile Include="DrtVisualTreeChangeSuite.cs" />
  </ItemGroup>
</Project>
