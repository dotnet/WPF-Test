<!-- test of shapes with DrawingBrush stroke with different Linecaps-->

<Canvas xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Background="#CCCCCC">

<Path Data="M 20 20 L 120 80 L 20 100 z" StrokeThickness="25" StrokeDashCap="Flat" StrokeDashArray="2 2">
<Path.Stroke>
<VisualBrush Viewport="0,0,20,20" ViewportUnits="Absolute" TileMode="Tile">
        <VisualBrush.Visual> 
                <Ellipse Width="51" Height="51" Fill="Blue" />
             </VisualBrush.Visual>
</VisualBrush>
</Path.Stroke>
</Path>

