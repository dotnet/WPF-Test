<!-- test of complement: The existing region is excluded from the new region-->
<Canvas xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
    <Path Fill="Blue">
        <Path.Data>
            <CombinedGeometry GeometryCombineMode="Exclude">
                <CombinedGeometry.Transform>
                    <TranslateTransform X="120" Y="50" />
                </CombinedGeometry.Transform>
                <CombinedGeometry.Geometry1>
                    <RectangleGeometry Rect="50 50 100 100" />
                </CombinedGeometry.Geometry1>
                <CombinedGeometry.Geometry2>
                    <RectangleGeometry Rect="0 0 100 100" />
                </CombinedGeometry.Geometry2>
            </CombinedGeometry>
        </Path.Data>
    </Path>
    <Path Fill="blue">
        <Path.Data>
            <CombinedGeometry GeometryCombineMode="
   Exclude">
                <CombinedGeometry.Transform>
                    <TranslateTransform X="50" Y="0" />
                </CombinedGeometry.Transform>
                <CombinedGeometry.Geometry1>
                    <EllipseGeometry Center="80 350" RadiusX="50" RadiusY="50" />
                </CombinedGeometry.Geometry1>
                <CombinedGeometry.Geometry2>
                    <EllipseGeometry Center="50 320" RadiusX="50" RadiusY="50" />
                </CombinedGeometry.Geometry2>
            </CombinedGeometry>
        </Path.Data>
    </Path>
    <Path Fill="blue" StrokeThickness="2" Stroke="black">
        <Path.Data>
            <CombinedGeometry GeometryCombineMode="
   Exclude">
                <CombinedGeometry.Transform>
                    <TranslateTransform X="250" Y="-5" />
                </CombinedGeometry.Transform>
                <CombinedGeometry.Geometry1>
                    <EllipseGeometry Center="50 350" RadiusX="100" RadiusY="100" />
                </CombinedGeometry.Geometry1>
                <CombinedGeometry.Geometry2>
                    <EllipseGeometry Center="50 350" RadiusX="50" RadiusY="50" />
                </CombinedGeometry.Geometry2>
            </CombinedGeometry>
        </Path.Data>
    </Path>
</Canvas>
