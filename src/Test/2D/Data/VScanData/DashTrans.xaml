<Canvas xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Decorator>
        <Decorator.RenderTransform>
            <TransformGroup>
                <TransformGroup.Children>
                    <TransformCollection>
                        <SkewTransform AngleY="0" />
                        <SkewTransform AngleX="20" />
                        <TranslateTransform X="-500" Y="50" />
                    </TransformCollection>
                </TransformGroup.Children>
            </TransformGroup>
        </Decorator.RenderTransform>
        <Rectangle Canvas.Left="500" Canvas.Top="50" Width="100" Height="100" StrokeDashCap="Triangle" StrokeThickness="10" StrokeDashOffset="0.5" StrokeDashArray="1 3 2 3 " Stroke="purple">
            <Rectangle.Fill>
                <RadialGradientBrush>
                    <GradientBrush.GradientStops>
                        <GradientStopCollection>
                            <GradientStop Color="red" Offset="0" />
                            <GradientStop Color="yellow" Offset="1" />
                        </GradientStopCollection>
                    </GradientBrush.GradientStops>
                </RadialGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
    </Decorator>
    <Decorator>
        <Decorator.RenderTransform>
            <TransformGroup>
                <TransformGroup.Children>
                    <TransformCollection>
                        <TranslateTransform X="100" Y="150" />
                        <SkewTransform AngleY="35" />
                        <SkewTransform AngleX="35" />
                    </TransformCollection>
                </TransformGroup.Children>
            </TransformGroup>
        </Decorator.RenderTransform>
        <Polygon Fill="yellow" Stroke="black" StrokeDashOffset="0.5" StrokeDashCap="Round" StrokeThickness="6" StrokeDashArray="1 2 1 2 1 2" Points="100,10 150,35 150,65 100,90 50,65 50,35">
        </Polygon>
    </Decorator>
    <Decorator>
        <Decorator.RenderTransform>
            <TransformGroup>
                <TransformGroup.Children>
                    <TransformCollection>
                        <RotateTransform Angle="20" />
                        <TranslateTransform X="350" Y="-100" />
                    </TransformCollection>
                </TransformGroup.Children>
            </TransformGroup>
        </Decorator.RenderTransform>
        <Path Data="M100,200 C100,100 250,100 250,200 S400,300 400,200" StrokeThickness="10" StrokeDashOffset="0.5" StrokeDashCap="Triangle" StrokeDashArray="1 2">
            <Path.Stroke>
                <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                    <GradientBrush.GradientStops>
                        <GradientStopCollection>
                            <GradientStop Color="red" Offset="0" />
                            <GradientStop Color="blue" Offset="1" />
                        </GradientStopCollection>
                    </GradientBrush.GradientStops>
                </LinearGradientBrush>
            </Path.Stroke>
        </Path>
    </Decorator>
    <Decorator>
        <Decorator.RenderTransform>
            <TransformGroup>
                <TransformGroup.Children>
                    <TransformCollection>
                        <RotateTransform Angle="60" />
                        <ScaleTransform ScaleX="1.5" ScaleY="1.5" />
                    </TransformCollection>
                </TransformGroup.Children>
            </TransformGroup>
        </Decorator.RenderTransform>
        <Path StrokeThickness="6">
            <Path.Data>
                <EllipseGeometry Center="250 75" RadiusX="75" RadiusY="50" />
            </Path.Data>
            <Path.Stroke>red</Path.Stroke>
            <Path.StrokeDashCap>Round</Path.StrokeDashCap>
            <Path.StrokeDashArray>1 3 1 3 1 3</Path.StrokeDashArray>
            <Path.StrokeDashOffset>2</Path.StrokeDashOffset>
            <Path.Fill>RadialGradient blue yellow</Path.Fill>
            <Path.Stroke>red</Path.Stroke>
        </Path>
    </Decorator>
    <Decorator>
        <Decorator.RenderTransform>
            <ScaleTransform ScaleX="2" ScaleY="2" />
        </Decorator.RenderTransform>
        <Line>
            <Line.X1>455</Line.X1>
            <Line.Y1>255</Line.Y1>
            <Line.X2>200</Line.X2>
            <Line.Y2>100</Line.Y2>
            <Line.Stroke>purple</Line.Stroke>
            <Line.StrokeThickness>7.5</Line.StrokeThickness>
            <Line.StrokeStartLineCap>Round</Line.StrokeStartLineCap>
            <Line.StrokeEndLineCap>Triangle</Line.StrokeEndLineCap>
            <Line.StrokeDashCap>Flat</Line.StrokeDashCap>
            <Line.StrokeDashArray>1 1</Line.StrokeDashArray>
            <Line.StrokeDashOffset>2</Line.StrokeDashOffset>
        </Line>
    </Decorator>
    <Decorator>
        <Decorator.RenderTransform>
            <TransformGroup>
                <TransformGroup.Children>
                    <TransformCollection>
                        <ScaleTransform ScaleX="2" ScaleY="2" />
                        <TranslateTransform X="200" Y="330" />
                    </TransformCollection>
                </TransformGroup.Children>
            </TransformGroup>
        </Decorator.RenderTransform>
        <Polygon FillRule="Nonzero" Stroke="black" StrokeDashOffset="0.5" StrokeDashCap="Round" StrokeThickness="2" StrokeDashArray="1 3 1 3 1 3" Points="48,16 16,96 96,48 0,48 80,96">
            <Polygon.Fill>
                <LinearGradientBrush StartPoint="0,0" EndPoint="1,0">
                    <GradientBrush.GradientStops>
                        <GradientStopCollection>
                            <GradientStop Color="red" Offset="0" />
                            <GradientStop Color="blue" Offset="1" />
                        </GradientStopCollection>
                    </GradientBrush.GradientStops>
                </LinearGradientBrush>
            </Polygon.Fill>
        </Polygon>
    </Decorator>
</Canvas>
