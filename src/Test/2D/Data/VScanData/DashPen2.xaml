<Canvas xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Rectangle Canvas.Left="500" Canvas.Top="50" Width="200" Height="200" StrokeDashCap="Round" StrokeThickness="10" StrokeDashOffset="0.5" StrokeDashArray="1 3 2 3 " Stroke="purple">
        <Rectangle.Fill>
            <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                <GradientBrush.GradientStops>
                    <GradientStopCollection>
                        <GradientStop Color="blue" Offset="0" />
                        <GradientStop Color="yellow" Offset="1" />
                    </GradientStopCollection>
                </GradientBrush.GradientStops>
            </LinearGradientBrush>
        </Rectangle.Fill>
    </Rectangle>
    <Polygon Fill="purple" Stroke="black" StrokeDashOffset="0.5" StrokeDashCap="Round" StrokeThickness="6" StrokeDashArray="1 3 1 3 1 3" Points="100,10 150,35 150,65 100,90 50,65 50,35">
    </Polygon>
    <Path Data="M100,200 C100,100 250,100 250,200 S400,300 400,200" StrokeThickness="16" StrokeDashOffset="0" StrokeDashCap="Flat" StrokeDashArray="2 2">
        <Path.Stroke>
            <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                <GradientBrush.GradientStops>
                    <GradientStopCollection>
                        <GradientStop Color="red" Offset="0" />
                        <GradientStop Color="blue" Offset="1" />
                    </GradientStopCollection>
                </GradientBrush.GradientStops>
            </LinearGradientBrush>
        </Path.Stroke>
    </Path>
    <Ellipse Width="156" Height="106" Canvas.Left="222" Canvas.Top="22" StrokeThickness="6">
        <Ellipse.Stroke>red</Ellipse.Stroke>
        <Ellipse.StrokeDashCap>Triangle</Ellipse.StrokeDashCap>
        <Ellipse.StrokeDashArray>1 3 1 3 1 3</Ellipse.StrokeDashArray>
        <Ellipse.StrokeDashOffset>2</Ellipse.StrokeDashOffset>
        <Ellipse.Fill>blue</Ellipse.Fill>
    </Ellipse>
    <Line>
        <Line.X1>455</Line.X1>
        <Line.Y1>255</Line.Y1>
        <Line.X2>200</Line.X2>
        <Line.Y2>100</Line.Y2>
        <Line.Stroke>green</Line.Stroke>
        <Line.StrokeThickness>10</Line.StrokeThickness>
        <Line.StrokeStartLineCap>Triangle</Line.StrokeStartLineCap>
        <Line.StrokeEndLineCap>Triangle</Line.StrokeEndLineCap>
        <Line.StrokeDashCap>Triangle</Line.StrokeDashCap>
        <Line.StrokeDashArray>0 2</Line.StrokeDashArray>
        <Line.StrokeDashOffset>0.5</Line.StrokeDashOffset>
    </Line>
</Canvas>
