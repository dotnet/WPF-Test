<Canvas xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Background="white">

<Ellipse Width="360" Height="170" Canvas.Left="120" Canvas.Top="90" StrokeThickness="10">
  
     
     
     
     
     <Ellipse.Stroke>red</Ellipse.Stroke>
     
     <Ellipse.StrokeDashCap>Triangle</Ellipse.StrokeDashCap>
     <Ellipse.StrokeDashArray>1 3 1 3 1 3</Ellipse.StrokeDashArray>
     <Ellipse.StrokeDashOffset>2</Ellipse.StrokeDashOffset>
     <Ellipse.Fill>blue</Ellipse.Fill>
     
</Ellipse>  

</Canvas>
