<DockPanel
   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
   Background="White"
>
    <FlowDocumentScrollViewer DockPanel.Dock="Top"><FlowDocument>
    <Table>
        <TableRowGroup><TableRow><TableCell>
		<Paragraph TextDecorations="Underline">
            		TextDecorations="Underline"
		</Paragraph>
	</TableCell></TableRow></TableRowGroup>
    </Table>
    </FlowDocument></FlowDocumentScrollViewer>
    <FlowDocumentScrollViewer DockPanel.Dock="Top"><FlowDocument>
    <Table>
        <TableRowGroup><TableRow><TableCell>
		<Paragraph TextDecorations="Strikethrough">
            		TextDecorations="Strikethrough"
        	</Paragraph>
	</TableCell></TableRow></TableRowGroup>
    </Table>
    </FlowDocument></FlowDocumentScrollViewer>
    <FlowDocumentScrollViewer DockPanel.Dock="Top"><FlowDocument>
    <Table>
        <TableRowGroup><TableRow><TableCell>
		<Paragraph TextDecorations="OverLine">
            		TextDecorations="OverLine"
		</Paragraph>
	</TableCell></TableRow></TableRowGroup>
    </Table>
    </FlowDocument></FlowDocumentScrollViewer>
</DockPanel>
