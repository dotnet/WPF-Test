<ContainmentSchema>
	<TextBlock>
		<RAWTEXT/>
		<INLINECONTENTWITHOUTANCHOREDBLOCKS/>
		<UIELEMENT/>
	</TextBlock>
	
	<FlowDocumentPageViewer>
		<FlowDocument/>
	</FlowDocumentPageViewer>
	
	<FlowDocumentScrollViewer>
		<FlowDocument/>
	</FlowDocumentScrollViewer>

	<FlowDocument>
		<BLOCKCONTENT/>
	</FlowDocument>

	<BLOCKCONTENT>
		<List Child="MUST HAVE"/>
		<Paragraph/>
		<Section/>
		<Table/>
        <BlockUIContainer/>
	</BLOCKCONTENT>

	<RAWTEXT>
		Some Plain Text		
	</RAWTEXT>

	<UIELEMENT>
		<Button/>
		<Grid/>		
    </UIELEMENT>

    <INLINECONTENTWITHOUTANCHOREDBLOCKS>
		<LineBreak/>
		<Run/>
		<InlineUIContainer/>
		<Span/>
		<Bold/>
		<Hyperlink/>
		<Italic/>
        <Underline/>
	</INLINECONTENTWITHOUTANCHOREDBLOCKS>

	<INLINECONTENT>
		<LineBreak/>
		<Run/>
		<InlineUIContainer/>
		<Figure/>
		<Floater/>
		<Span/>
		<Bold/>
		<Hyperlink/>
		<Italic/>
        <Underline/>
	</INLINECONTENT>

    <VALIDHYPERLINKCHILDREN>
		<LineBreak/>
		<Run/>
		<InlineUIContainer/>
		<Span/>
		<Bold/>
		<Italic/>
        <Underline/>
    </VALIDHYPERLINKCHILDREN>

	<InlineUIContainer>
		<Button/>
	</InlineUIContainer>

	<Figure>
		<BLOCKCONTENT/>
	</Figure>

	<Floater>
		<BLOCKCONTENT/>
	</Floater>

	<Span>
		<RAWTEXT/>
		<INLINECONTENT/>
		<UIELEMENT/>
	</Span>

	<List>
		<ListItem/>
	</List>

	<ListItem>
		<BLOCKCONTENT/>
	</ListItem>
	
	<Paragraph>
		<RAWTEXT/>
		<INLINECONTENT/>
		<UIELEMENT/>
	</Paragraph>

	<Section>
		<BLOCKCONTENT/>
	</Section>

	<Table>
		<Table.Columns />
		<TableRowGroup/>
	</Table>

	<TableRowGroup>
		<TableRow/>
	</TableRowGroup>

	<TableRow>
		<TableCell/>
	</TableRow>

	<TableCell>
		<BLOCKCONTENT/>
	</TableCell>
        
        <Table.Columns>
            <TableColumn/>
        </Table.Columns>

	<Bold>
		<RAWTEXT/>
		<INLINECONTENT/>
		<UIELEMENT/>
	</Bold>

	<Hyperlink>
		<RAWTEXT/>
		<VALIDHYPERLINKCHILDREN/>
		<UIELEMENT/>
	</Hyperlink>

	<Italic>
		<RAWTEXT/>
		<INLINECONTENT/>
		<UIELEMENT/>
	</Italic>

	<Underline>
		<RAWTEXT/>
		<INLINECONTENT/>
		<UIELEMENT/>
        </Underline>
        
    <BlockUIContainer>
        <Button/>        
    </BlockUIContainer>
</ContainmentSchema>
