<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <NoTargets>true</NoTargets>
  </PropertyGroup>

  <ItemGroup>
    <None Include="MASTERS\VSCAN\*.tif">
      <Link>MASTERS\VSCAN\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="MASTERS\PROPERTYDUMP\*.lxml">
      <Link>MASTERS\PROPERTYDUMP\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="Markup\*.xaml">
      <Link>Content\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="Markup\*.html">
      <Link>Content\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="Images\*.*">
      <Link>Content\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="SchemaXml\*.xml*">
      <Link>Content\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>