<Border xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Background="LightSteelBlue">
<Grid Width="1200">
  
  <Grid.Resources>
    <Style x:Key="{x:Type TextBlock}" TargetType="{x:Type TextBlock}">
      <Setter Property="FontFamily" Value="Tahoma" />
    </Style>
  </Grid.Resources>
  
  <Grid.ColumnDefinitions>
  <ColumnDefinition Width="4" />
  <ColumnDefinition />
  <ColumnDefinition Width="4" />
  <ColumnDefinition />
  <ColumnDefinition Width="4" />
  <ColumnDefinition />
  <ColumnDefinition Width="4" />
  </Grid.ColumnDefinitions>
  <Grid.RowDefinitions>
  <RowDefinition Height="4" />
  <RowDefinition Height="Auto" />
  <RowDefinition Height="4" />
  <RowDefinition Height="Auto" />
  <RowDefinition Height="4" />
  <RowDefinition Height="Auto" />
  <RowDefinition Height="4" />
  <RowDefinition Height="Auto" />
  <RowDefinition Height="4" />
  <RowDefinition Height="Auto" />
  <RowDefinition Height="4" />
  <RowDefinition Height="Auto" />
  <RowDefinition Height="4" />
  <RowDefinition Height="Auto" />
  <RowDefinition Height="4" />
  <RowDefinition Height="Auto" />
  <RowDefinition Height="4" />
  <RowDefinition Height="Auto" />
  <RowDefinition Height="4" />
  <RowDefinition Height="Auto" />
  <RowDefinition Height="4" />
  </Grid.RowDefinitions>


  <Border Background="White" Grid.Column="1" Grid.Row="3">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="10pt" LineHeight="Auto">
    Line stacking test: Line height = 
    <Bold xml:space="preserve" FontFamily="Courier New" FontSize="14pt" Foreground="Navy">[ default ]</Bold>
    # Line stacking strategy = 
    <Bold xml:space="preserve" FontFamily="Courier New" Foreground="Navy">[ default ]</Bold>.
  </TextBlock>
  </Border>

  <Border Background="White" Grid.Column="3" Grid.Row="3">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="10pt" LineHeight="Auto" LineStackingStrategy="MaxHeight">
    Line stacking test: Line height = 
    <Bold xml:space="preserve" FontFamily="Courier New" FontSize="14pt" Foreground="Navy">[ default ]</Bold>
    # Line stacking strategy = 
    <Bold xml:space="preserve" FontFamily="Courier New" Foreground="Navy">[ MaxHeight (default) ]</Bold>.
  </TextBlock>
  </Border>

  <Border Background="White" Grid.Column="5" Grid.Row="3">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="10pt" LineHeight="Auto" LineStackingStrategy="BlockLineHeight">
    Line stacking test: Line height = 
    <Bold xml:space="preserve" FontFamily="Courier New" FontSize="14pt" Foreground="Navy">[ default ]</Bold>
    # Line stacking strategy = 
    <Bold xml:space="preserve" FontFamily="Courier New" Foreground="Navy">[ BlockLineHeight ]</Bold>.
  </TextBlock>
  </Border>

  <Border Background="White" Grid.Column="1" Grid.Row="7">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="10pt" LineHeight="8pt">
    Line stacking test: Line height = 
    <Bold xml:space="preserve" FontFamily="Courier New" FontSize="14pt" Foreground="Navy">[ 8pt     ]</Bold>
    # Line stacking strategy = 
    <Bold xml:space="preserve" FontFamily="Courier New" Foreground="Navy">[ default ]</Bold>.
  </TextBlock>
  </Border>


  <Border Background="White" Grid.Column="3" Grid.Row="7">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="10pt" LineHeight="8pt" LineStackingStrategy="MaxHeight">
    Line stacking test: Line height = 
    <Bold xml:space="preserve" FontFamily="Courier New" FontSize="14pt" Foreground="Navy">[ 8pt     ]</Bold>
    # Line stacking strategy = 
    <Bold xml:space="preserve" FontFamily="Courier New" Foreground="Navy">[ MaxHeight (default) ]</Bold>.
  </TextBlock>
  </Border>


  <Border Background="White" Grid.Column="5" Grid.Row="7">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="10pt" LineHeight="8pt" LineStackingStrategy="BlockLineHeight">
    Line stacking test: Line height = 
    <Bold xml:space="preserve" FontFamily="Courier New" FontSize="14pt" Foreground="Navy">[ 8pt     ]</Bold>
    # Line stacking strategy = 
    <Bold xml:space="preserve" FontFamily="Courier New" Foreground="Navy">[ BlockLineHeight ]</Bold>.
  </TextBlock>
  </Border>

  <Border Background="White" Grid.Column="1" Grid.Row="11">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="10pt" LineHeight="20pt">
    Line stacking test: Line height = 
    <Bold xml:space="preserve" FontFamily="Courier New" FontSize="14pt" Foreground="Navy">[ 20pt    ]</Bold>
    # Line stacking strategy = 
    <Bold xml:space="preserve" FontFamily="Courier New" Foreground="Navy">[ default ]</Bold>.
  </TextBlock>
  </Border>

  <Border Background="White" Grid.Column="3" Grid.Row="11">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="10pt" LineHeight="20pt" LineStackingStrategy="MaxHeight">
    Line stacking test: Line height = 
    <Bold xml:space="preserve" FontFamily="Courier New" FontSize="14pt" Foreground="Navy">[ 20pt    ]</Bold>
    # Line stacking strategy = 
    <Bold xml:space="preserve" FontFamily="Courier New" Foreground="Navy">[ MaxHeight (default) ]</Bold>.
  </TextBlock>
  </Border>

  <Border Background="White" Grid.Column="5" Grid.Row="11">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="10pt" LineHeight="20pt" LineStackingStrategy="BlockLineHeight">
    Line stacking test: Line height = 
    <Bold xml:space="preserve" FontFamily="Courier New" FontSize="14pt" Foreground="Navy">[ 20pt    ]</Bold>
    # Line stacking strategy = 
    <Bold xml:space="preserve" FontFamily="Courier New" Foreground="Navy">[ BlockLineHeight ]</Bold>.
  </TextBlock>
  </Border>

  <Border Background="White" Grid.Column="1" Grid.Row="13">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="16pt">
    <Span FontSize="12pt" Background="Beige">
      <Bold FontSize="10pt" FontFamily="Courier New" Background="LightBlue" BaselineAlignment="Baseline">Baseline   </Bold>
      baseline
    </Span>
    alignment. <Span BaselineAlignment="Center"><Border Background="DarkBlue" Width="15" Height="50" /></Span>
    <Span FontSize="18pt" Background="Beige">
      <Bold FontSize="20pt" FontFamily="Courier New" Background="LightBlue" BaselineAlignment="Baseline">Baseline   </Bold>
      baseline
    </Span>
    alignment.
  </TextBlock>
  </Border>

  <Border Background="White" Grid.Column="3" Grid.Row="13">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="16pt">
    <Span FontSize="12pt" Background="Beige">
      <Bold FontSize="10pt" FontFamily="Courier New" Background="LightBlue" BaselineAlignment="Center">Center     </Bold>
      baseline
    </Span>
    alignment. <Span BaselineAlignment="Center"><Border Background="DarkBlue" Width="15" Height="50" /></Span>
    <Span FontSize="18pt" Background="Beige">
      <Bold FontSize="20pt" FontFamily="Courier New" Background="LightBlue" BaselineAlignment="Center">Center     </Bold>
      baseline
    </Span>
    alignment.
  </TextBlock>
  </Border>

  <Border Background="White" Grid.Column="1" Grid.Row="15">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="16pt">
    <Span FontSize="12pt" Background="Beige">
      <Bold FontSize="10pt" FontFamily="Courier New" Background="LightBlue" BaselineAlignment="Top">Top        </Bold>
      baseline
    </Span>
    alignment. <Span BaselineAlignment="Center"><Border Background="DarkBlue" Width="15" Height="50" /></Span>
    <Span FontSize="18pt" Background="Beige">
      <Bold FontSize="20pt" FontFamily="Courier New" Background="LightBlue" BaselineAlignment="Top">Top        </Bold>
      baseline
    </Span>
    alignment.
  </TextBlock>
  </Border>

  <Border Background="White" Grid.Column="3" Grid.Row="15">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="16pt">
    <Span FontSize="12pt" Background="Beige">
      <Bold FontSize="10pt" FontFamily="Courier New" Background="LightBlue" BaselineAlignment="Bottom">Bottom     </Bold>
      baseline
    </Span>
    alignment. <Span BaselineAlignment="Center"><Border Background="DarkBlue" Width="15" Height="50" /></Span>
    <Span FontSize="18pt" Background="Beige">
      <Bold FontSize="20pt" FontFamily="Courier New" Background="LightBlue" BaselineAlignment="Bottom">Bottom     </Bold>
      baseline
    </Span>
    alignment.
  </TextBlock>
  </Border>

  <Border Background="White" Grid.Column="1" Grid.Row="17">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="16pt">
    <Span FontSize="12pt" Background="Beige">
      <Bold FontSize="10pt" FontFamily="Courier New" Background="LightBlue" BaselineAlignment="TextTop">TextTop    </Bold>
      baseline
    </Span>
    alignment. <Span BaselineAlignment="Center"><Border Background="DarkBlue" Width="15" Height="50" /></Span>
    <Span FontSize="18pt" Background="Beige">
      <Bold FontSize="20pt" FontFamily="Courier New" Background="LightBlue" BaselineAlignment="TextTop">TextTop    </Bold>
      baseline
    </Span>
    alignment.
  </TextBlock>
  </Border>

  <Border Background="White" Grid.Column="3" Grid.Row="17">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="16pt">
    <Span FontSize="12pt" Background="Beige">
      <Bold FontSize="10pt" FontFamily="Courier New" Background="LightBlue" BaselineAlignment="TextBottom">TextBottom </Bold>
      baseline
    </Span>
    alignment. <Span BaselineAlignment="Center"><Border Background="DarkBlue" Width="15" Height="50" /></Span>
    <Span FontSize="18pt" Background="Beige">
      <Bold FontSize="20pt" FontFamily="Courier New" Background="LightBlue" BaselineAlignment="TextBottom">TextBottom </Bold>
      baseline
    </Span>
    alignment.
  </TextBlock>
  </Border>

  <Border Background="White" Grid.Column="1" Grid.Row="19">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="16pt">
    <Span FontSize="12pt" Background="Beige">
      <Bold FontSize="10pt" FontFamily="Courier New" Background="LightBlue" BaselineAlignment="Subscript">Subscript  </Bold>
      baseline
    </Span>
    alignment. <Span BaselineAlignment="Center"><Border Background="DarkBlue" Width="15" Height="50" /></Span>
    <Span FontSize="18pt" Background="Beige">
      <Bold FontSize="20pt" FontFamily="Courier New" Background="LightBlue" BaselineAlignment="Subscript">Subscript  </Bold>
      baseline
    </Span>
    alignment.
  </TextBlock>
  </Border>

  <Border Background="White" Grid.Column="3" Grid.Row="19">
  <TextBlock TextWrapping="WrapWithOverflow" FontSize="16pt">
    <Span FontSize="12pt" Background="Beige">
      <Bold FontSize="10pt" FontFamily="Courier New" Background="LightBlue" BaselineAlignment="Superscript">Superscript</Bold>
      baseline
    </Span>
    alignment. <Span BaselineAlignment="Center"><Border Background="DarkBlue" Width="15" Height="50" /></Span>
    <Span FontSize="18pt" Background="Beige">
      <Bold FontSize="20pt" FontFamily="Courier New" Background="LightBlue" BaselineAlignment="Superscript">Superscript</Bold>
      baseline
    </Span>
    alignment.
  </TextBlock>
  </Border>

</Grid>
</Border>
