    <UICulture>en-us</UICulture>
  </PropertyGroup>
  <ItemGroup>
    <Page Include="DataTrigger.xaml" />
  </ItemGroup>
  <ItemGroup>
    <ApplicationDefinition Include="DrtDataSrvMSBApp.xaml" />
    <Compile Include="DrtDataSrvMSBApp.xaml.cs" />
    <Compile Include="DataTrigger.xaml.cs" />
    <Compile Remove="$(WpfTestBasePath)\..\CommonAssemblyInfo.cs" />
    <Compile Include="Assemblyinfo.cs" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="DrtFiles\DataSrvMSB\Content.JPG">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
