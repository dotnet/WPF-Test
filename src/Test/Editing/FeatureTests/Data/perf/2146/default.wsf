<JOB ID="main">
<SCRIPT LANGUAGE="JScript" SRC="..\..\include.js" />

<SCRIPT LANGUAGE="JScript">
var oTestProcess = new Process();

var sCommandLine = "\"" + TESTBIN_PATH +  "\\WCP.EditingPerf.exe\" /Scenario=DocumentLoad /TargetControl=RichTextBox /InstanceCount=10 /PerfMeasurement=WorkingSet";
g_oShell.CurrentDirectory = TESTBIN_PATH ; 

ensureCScript();

printBanner("Editing - DocumentLoad for RichTextBox x 10 paragraphs (WS)");

warmUpApp(sCommandLine);

if (oTestProcess.start(sCommandLine))
{
    WScript.Sleep(10000);
    runAvWkSet(oTestProcess.processID);
    oTestProcess.kill();
}

</SCRIPT>
</JOB>
