          <WindowsFormsHost x:Name="wfhFGYellowBlueRad" Foreground="{StaticResource MyYellowBlueRad}">
            <wf:Button x:Name="wf_btn3_6" Text="Pass if foreground shown is Yellow/Blue Radial"/>
