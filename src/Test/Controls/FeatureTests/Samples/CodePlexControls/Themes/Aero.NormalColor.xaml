<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <!--
    Do not merge other theme specfic control dictionaries into this one. An example would be
    CustomControl.aero.normalcolor.xaml. Those controls should be merged into their own theme
    dictionary such as aero.normalcolor.xaml.
  -->
  <ResourceDictionary.MergedDictionaries>

    <!--
      If there are multiple controls contained in this library, add the generics for the extra
      ones here.
    -->

    <!--
      We want to reuse the style definition in Luna.NormalColor theme
    -->
    <ResourceDictionary Source="CodePlexControls;Component/themes/NumericUpDown.Luna.NormalColor.xaml"/>

  </ResourceDictionary.MergedDictionaries>

</ResourceDictionary>
