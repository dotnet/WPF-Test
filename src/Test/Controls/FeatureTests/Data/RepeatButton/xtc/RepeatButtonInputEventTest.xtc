      <Keyword>Localization_Suite</Keyword>
    </Keywords>
    <DATA>
      <INIT Class="ButtonInputEventTest"/>
      <PICT Name="ButtonInputTest.pict" Args="/o:2" Excludes=""/>
      <XAMLS>
        <XAML Name="ButtonBVT.xaml"/>
        <XAML Name="ButtonClickModePressBVT.xaml"/>
      </XAMLS>
      <VARIATION ControlName="repeatButton" InputType="Keyboard" EventName="Click" ShouldEventFire="true" />
      <VARIATION ControlName="repeatButton" InputType="Mouse" EventName="Click" ShouldEventFire="true" MouseButton="Left" />
    </DATA>
  </TEST>
</XTC>
