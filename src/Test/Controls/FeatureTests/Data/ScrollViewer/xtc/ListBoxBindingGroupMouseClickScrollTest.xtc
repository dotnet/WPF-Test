<XTC>
  <TEST Name="ListBoxMouseClickScrollTest" Priority="1" SubArea="ScrollViewer" Timeout="600">
    <DATA>
      <INIT Class="ListBoxScrollingTest"/>
      <PICT Name="ListBoxScrollingTest.pict" Args="/o:2" Excludes=""/>
      <XAMLS>
        <XAML Name="ListBoxBindXmlDataIsLandsGroupingScrollingTest.xaml"/>
      </XAMLS>
      <VARIATION ControlName="listbox" InputType="Mouse" Orientation="Vertical" ScrollingMode="LineDown" EventName="ScrollChanged" ShouldEventFire="true" MouseButton="Left" />
      <VARIATION ControlName="listbox" InputType="Mouse" Orientation="Vertical" ScrollingMode="PageDown" EventName="ScrollChanged" ShouldEventFire="true" MouseButton="Left" />
      <VARIATION ControlName="listbox" InputType="Mouse" Orientation="Vertical" ScrollingMode="LineUp" EventName="ScrollChanged" ShouldEventFire="true" MouseButton="Left" />
    </DATA>
  </TEST>
</XTC>
