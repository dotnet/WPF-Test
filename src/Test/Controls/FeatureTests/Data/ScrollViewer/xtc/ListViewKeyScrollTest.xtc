<XTC>
  <TEST Name="ListBoxMouseClickScrollTest" Priority="0" SubArea="ScrollViewer" Timeout="600">
    <DATA>
      <INIT Class="ListBoxScrollingTest"/>
      <PICT Name="ListBoxScrollingTest.pict" Args="/o:2" Excludes=""/>
      <XAMLS>
        <XAML Name="ListBoxBindLargeDataScrollingTest.xaml"/>
        <XAML Name="ListBoxBindSmallDataScrollingTest.xaml"/>
        <XAML Name="ListBoxScrollingTest.xaml"/>
      </XAMLS>
      <VARIATION ControlName="listview" InputType="Keyboard" Orientation="Vertical" EventName="ScrollChanged" ShouldEventFire="true" Key="Down" />
      <VARIATION ControlName="listview" InputType="Keyboard" Orientation="Vertical" EventName="ScrollChanged" ShouldEventFire="true" Key="Up" />
      <VARIATION ControlName="listview" InputType="Keyboard" Orientation="Vertical" EventName="ScrollChanged" ShouldEventFire="true" Key="PageUp" />
    </DATA>
  </TEST>
</XTC>
