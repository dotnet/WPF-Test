<XTC>
  <TEST Name="ListBoxMouseDragScrollTest" Priority="1" SubArea="ScrollViewer" Timeout="600" Disabled="True">
    <DATA>
      <INIT Class="ListBoxScrollingTest"/>
      <PICT Name="ListBoxScrollingTest.pict" Args="/o:2" Excludes="1,2,6"/>
      <XAMLS>
        <XAML Name="ListBoxBindXmlDataIsLandsGroupingScrollingTest.xaml"/>
        <XAML Name="ListBoxBindXmlDataIsLandsSortingScrollingTest.xaml"/>
      </XAMLS>
      <VARIATION ControlName="listbox" InputType="MouseDrag" Orientation="Vertical" EventName="ScrollChanged" DragThumbScenario="TopToBottom" />
      <VARIATION ControlName="listbox" InputType="MouseDrag" Orientation="Vertical" EventName="ScrollChanged" DragThumbScenario="TopToMiddle" />
      <VARIATION ControlName="listbox" InputType="MouseDrag" Orientation="Vertical" EventName="ScrollChanged" DragThumbScenario="BottomToTop" />
      <VARIATION ControlName="listbox" InputType="MouseDrag" Orientation="Vertical" EventName="ScrollChanged" DragThumbScenario="BottomToMiddle" />
    </DATA>
  </TEST>
</XTC>
