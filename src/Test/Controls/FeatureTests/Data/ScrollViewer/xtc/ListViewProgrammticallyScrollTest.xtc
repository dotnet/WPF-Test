<XTC>
  <TEST Name="ListBoxProgrammticallyScrollTest" Priority="0" SubArea="ScrollViewer" Timeout="600">
    <DATA>
      <INIT Class="ListBoxScrollingTest"/>
      <PICT Name="ListBoxScrollingTest.pict" Args="/o:2" Excludes=""/>
      <XAMLS>
        <XAML Name="ListBoxBindLargeDataScrollingTest.xaml"/>
        <XAML Name="ListBoxBindSmallDataScrollingTest.xaml"/>
        <XAML Name="ListBoxScrollingTest.xaml"/>
      </XAMLS>
      <VARIATION ControlName="listview" InputType="Code" Orientation="Vertical" ScrollingMode="LineDown" EventName="ScrollChanged" ShouldEventFire="true" />
      <VARIATION ControlName="listview" InputType="Code" Orientation="Vertical" ScrollingMode="PageDown" EventName="ScrollChanged" ShouldEventFire="true" />
      <VARIATION ControlName="listview" InputType="Code" Orientation="Vertical" ScrollingMode="LineUp" EventName="ScrollChanged" ShouldEventFire="true" />
      <VARIATION ControlName="listview" InputType="Code" Orientation="Vertical" ScrollingMode="PageUp" EventName="ScrollChanged" ShouldEventFire="true" />
    </DATA>
  </TEST>
</XTC>
