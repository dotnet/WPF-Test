<XTC>
  <TEST	Name="MenuKeySelectMenuItemBVT"	Priority="0" SubArea="Menu" ExecutionGroup="BVT">
    <DATA>
      <INIT Class="MenuKeySelectMenuItemBVT"/>
      <VARIATION ID="1">
        <CONTROL>
          <Menu
              xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
              Width="30" HorizontalAlignment="Left" Background="White">
            <MenuItem Header="_File">
              <MenuItem Header="_New" />
              <MenuItem Header="_Open" />
              <Separator/>
              <MenuItem Header="Menu with Submenu">
                <MenuItem Header="_submenuitem1"/>
                <MenuItem Header="_submenuitem2">
                  <MenuItem Header="_submenuitem2.1"/>
                </MenuItem>
              </MenuItem>
              <Separator/>
              <MenuItem Header="_Close"/>
              <MenuItem Header="_MenuItem">
              </MenuItem>
            </MenuItem>
          </Menu>
        </CONTROL>
      </VARIATION>
    </DATA>
  </TEST>
</XTC>
