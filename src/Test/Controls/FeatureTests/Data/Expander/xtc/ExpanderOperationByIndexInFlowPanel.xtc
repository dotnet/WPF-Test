<XTC>
  <TEST Name="Expander: Testing accessing Expander in panel by index" SubArea="Expander" Priority="2" ExecutionGroup="Pri2">
    <DATA>
      <INIT Class="ExpanderOperationByIndexInFlowPanel" SuppressGtoLogger="true"/>
      <VARIATION ID="2">
        <CONTROL>
          <DockPanel xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
            <Expander ExpandDirection="Down" DockPanel.Dock="Top"  Name="expander">
              <Expander.Header>Header</Expander.Header>
              <Expander.Content>Content</Expander.Content>
            </Expander>
            <Expander ExpandDirection="Up"  DockPanel.Dock="Top" Name="expander1">
              <Expander.Header>Header</Expander.Header>
              <Expander.Content>Content</Expander.Content>
            </Expander>
          </DockPanel>
        </CONTROL>
      </VARIATION>
      <VARIATION ID="3">
        <CONTROL>
          <StackPanel xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
            <Expander ExpandDirection="Right" DockPanel.Dock="Top" IsExpanded="true" Name="expander">
              <Expander.Header>Header</Expander.Header>
              <Expander.Content>Content</Expander.Content>
            </Expander>
            <Expander ExpandDirection="Right" DockPanel.Dock="Top" Name="expander1">
              <Expander.Header>Header</Expander.Header>
              <Expander.Content>Content</Expander.Content>
            </Expander>
          </StackPanel>
        </CONTROL>
      </VARIATION>
    </DATA>
  </TEST>
</XTC>



