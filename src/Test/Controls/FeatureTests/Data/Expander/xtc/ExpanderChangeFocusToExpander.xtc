<XTC>
  <TEST Name="Expander: Testing changing focus from one Expander to another one" SubArea="Expander" Priority="2" ExecutionGroup="Pri2">
    <DATA>
      <INIT Class="ExpanderChangeFocusToExpander" SuppressGtoLogger="true"/>
      <VARIATION ID="1">
        <CONTROL>
          <StackPanel xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
            <Expander Name="expander" Header="Header"
                    ExpandDirection="Down"
                    IsExpanded="false"
                    >
              <Expander.Content>
                <Button Name="btn"/>
              </Expander.Content>
            </Expander>
            <Expander Name="expander1" Header="Header" Content="Content"/>
          </StackPanel >
        </CONTROL>
      </VARIATION>
    </DATA>
  </TEST>
</XTC>




