<XTC>
  <TEST Name="Expander: Testing DP for Content" SubArea="Expander" Priority="2" ExecutionGroup="Pri2" Disabled="True">
    <DATA>
      <INIT Class="ControlTestActionValidationUnitTest" SuppressGtoLogger="true"/>
      <VARIATION ID="1">
        <CONTROL>
          <Expander Name="expander" Header="Header" Content="Content" HorizontalContentAlignment="Center"
                  ExpandDirection="Down"
                  IsExpanded="false"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    />
        </CONTROL>
        <Actions />
        <Validations>
          <Validation Name="CompareDPValidation">
            <Parameter Value="ExpandSite" />
            <Parameter Value="expander" />
            <Parameter Value="HorizontalAlignment" />
            <Parameter Value="HorizontalContentAlignment" />
          </Validation>
        </Validations>
      </VARIATION>
      <VARIATION ID="2">
        <CONTROL>
          <Expander Name="expander" Header="Header" Content="Content" VerticalContentAlignment="Center"

                  ExpandDirection="Down"
                  IsExpanded="false"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    />
        </CONTROL>
        <Actions />
        <Validations>
          <Validation Name="CompareDPValidation">
            <Parameter Value="ExpandSite" />
            <Parameter Value="expander" />
            <Parameter Value="VerticalAlignment" />
            <Parameter Value="VerticalContentAlignment" />
          </Validation>
        </Validations>
      </VARIATION>
    </DATA>
  </TEST>
</XTC>





