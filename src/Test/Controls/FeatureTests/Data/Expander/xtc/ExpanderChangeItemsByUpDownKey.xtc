<XTC>
  <TEST Name="Expander: Testing changing focus by Up,Down keys" SubArea="Expander" Priority="2" ExecutionGroup="Pri2">
    <DATA>
      <INIT Class="ExpanderChangeItemsByUpDownKey" SuppressGtoLogger="true"/>
      <VARIATION ID="1">
        <CONTROL>
          <StackPanel xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
           <Expander Name="expander" Header="Header"
                    ExpandDirection="Down"
                    IsExpanded="true"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
              <Expander.Content>
                <StackPanel>
                  <Button Name="control1" />
                  <Button Name="control2" />
                </StackPanel>
              </Expander.Content>
            </Expander>
            <Expander Name="expander1" Header="Header" Content="Content"
                    ExpandDirection="Down"
                    IsExpanded="true"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    />
          </StackPanel>
        </CONTROL>
      </VARIATION>
    </DATA>
  </TEST>
</XTC>
