<XTC>
  <TEST Name="CheckBoxInputEventTest" Priority="0" SubArea="CheckBox" Timeout="600">
    <Keywords>
      <Keyword>Localization_Suite</Keyword>
    </Keywords>
    <DATA>
      <INIT Class="ButtonInputEventTest"/>
      <PICT Name="ButtonInputTest.pict" Args="/o:2" Excludes=""/>
      <XAMLS>
        <XAML Name="ButtonBVT.xaml"/>
        <XAML Name="ButtonClickModePressBVT.xaml"/>
      </XAMLS>
      <VARIATION ControlName="checkBox" InputType="Keyboard" EventName="Click" ShouldEventFire="true" Key="Space" />
      <VARIATION ControlName="uncheckedCheckBox" InputType="Keyboard" EventName="Checked" ShouldEventFire="true" Key="Space" />
      <VARIATION ControlName="checkedCheckBox" InputType="Keyboard" EventName="Unchecked" ShouldEventFire="true" Key="Space" />
      <VARIATION ControlName="uncheckedCheckBoxThreeState" InputType="Keyboard" EventName="Checked" ShouldEventFire="true" Key="Space" />
      <VARIATION ControlName="checkedCheckBoxThreeState" InputType="Keyboard" EventName="Indeterminate" ShouldEventFire="true" Key="Space" />
      <VARIATION ControlName="nullCheckedCheckBoxThreeState" InputType="Keyboard" EventName="Unchecked" ShouldEventFire="true" Key="Space" />

      <VARIATION ControlName="checkBox" InputType="Mouse" EventName="Click" ShouldEventFire="true" MouseButton="Left" />
      <VARIATION ControlName="uncheckedCheckBox" InputType="Mouse" EventName="Checked" ShouldEventFire="true" MouseButton="Left" />
      <VARIATION ControlName="checkedCheckBox" InputType="Mouse" EventName="Unchecked" ShouldEventFire="true" MouseButton="Left" />
      <VARIATION ControlName="uncheckedCheckBoxThreeState" InputType="Mouse" EventName="Checked" ShouldEventFire="true" MouseButton="Left" />
      <VARIATION ControlName="checkedCheckBoxThreeState" InputType="Mouse" EventName="Indeterminate" ShouldEventFire="true" MouseButton="Left" />
      <VARIATION ControlName="nullCheckedCheckBoxThreeState" InputType="Mouse" EventName="Unchecked" ShouldEventFire="true" MouseButton="Left" />
    </DATA>
  </TEST>
</XTC>
