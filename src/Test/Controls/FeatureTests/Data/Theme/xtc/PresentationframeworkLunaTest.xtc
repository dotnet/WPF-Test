<?xml version="1.0"?>
<XTC>
  <TEST Name="PresentationframeworkLunaTest:Control in Markup" SubArea="Theme" Priority="2" ExecutionGroup="Pri2">
    <DATA>
      <INIT Class="PresentationframeworkLunaTest2" SuppressGtoLogger = "true"/>

      <VARIATION ID="1">
        <Button
