<XTC>
  <TEST Name="StatusBar:ToolTip" SubArea="StatusBar" Priority="2" ExecutionGroup="Pri2">
    <DATA>
      <INIT Class="StatusBarToolTip"/>
      <VARIATION ID="1">
        <CONTROL>
          <StatusBar
                  Height="30"
                  IsEnabled="false"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    />
        </CONTROL>
      </VARIATION>
      <VARIATION ID="2">
        <CONTROL>
          <StatusBar
                  Height="30"
                  IsEnabled="true"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    />
        </CONTROL>
      </VARIATION>
    </DATA>
  </TEST>
</XTC>
