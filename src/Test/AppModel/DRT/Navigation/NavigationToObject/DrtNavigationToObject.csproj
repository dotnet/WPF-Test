    <AssemblyName>DrtNavigationToObject</AssemblyName>
    <LinkSubsystem>console</LinkSubsystem>
    <OutputType>EXE</OutputType>
    <GenerateAssemblyAttribute>false</GenerateAssemblyAttribute>
    <TestCode>true</TestCode>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="DrtnavigationToObject.cs" />
    <Compile Include="..\..\Logger\Logger.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="FlowDocument.xaml">
      <Link>DrtFiles\NavigationToObject\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>
