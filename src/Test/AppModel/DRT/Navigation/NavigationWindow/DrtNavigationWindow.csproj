<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>DrtNavigationWindow</AssemblyName>
    <LinkSubsystem>console</LinkSubsystem>
    <AssemblyIdentityVersionName>WCP</AssemblyIdentityVersionName>
    <OutputType>EXE</OutputType>
    <GenerateAssemblyAttribute>false</GenerateAssemblyAttribute>
    <TestCode>true</TestCode>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Drtnavigationwindow.cs" />
    <Compile Include="..\..\Logger\Logger.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="NavWindowBar.xaml">
      <Link>DrtFiles\NavigationWindow\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="NavWindowBar2.xaml">
      <Link>DrtFiles\NavigationWindow\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="NavWindowFoo.xaml">
      <Link>DrtFiles\NavigationWindow\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="NavWindowFoo2.xaml">
      <Link>DrtFiles\NavigationWindow\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>
