<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="Current">
  <PropertyGroup>
    <AssemblyName>DrtColorAPI</AssemblyName>
    <AssemblyIdentityVersionName>WCP</AssemblyIdentityVersionName>
    <OutputType>EXE</OutputType>
    <GenerateAssemblyAttribute>false</GenerateAssemblyAttribute>
    <LinkSubsystem>console</LinkSubsystem>
    <TestCode>true</TestCode>
    <FileUpgradeFlags>
    </FileUpgradeFlags>
    <UpgradeBackupLocation>
    </UpgradeBackupLocation>
    <OldToolsVersion>2.0</OldToolsVersion>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="DrtBase.cs" />
    <Compile Include="DrtColorAPI.cs" />
    <Compile Include="DrtPrimitives.cs" />
    <None Include="testCMYK1.icc">
      <Link>DrtFiles\DrtColorAPI\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>
