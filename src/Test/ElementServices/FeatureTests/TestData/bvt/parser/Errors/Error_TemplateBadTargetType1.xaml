<Canvas
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
>
    <Canvas.Resources>
        <Style x:Key="test" TargetType="Rectangle">
            <Setter Property="Fill" Value="Blue" />
        </Style>

    </Canvas.Resources>

    <Rectangle Style="{StaticResource test}" Canvas.Top="50"
        Canvas.Left="50" Width="50" Height="50" />

</Canvas>
