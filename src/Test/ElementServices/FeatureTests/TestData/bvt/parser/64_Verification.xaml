



<cmn:CustomDockPanel xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mil="clr-namespace:System.Windows.Media;assembly=PresentationCore"
 xmlns:cc="clr-namespace:Avalon.Test.CoreUI.Parser;assembly=CoreTestsUntrusted" xmlns:cmn="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime" xmlns:MappingPIGen1="clr-namespace:System.Windows.Media;assembly=PresentationCore" xmlns:MappingPIGen2="clr-namespace:Avalon.Test.CoreUI.Parser;assembly=CoreTestsUntrusted" xmlns:MappingPIGen3="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime">
    <cmn:CustomDockPanel.Resources>
        <SolidColorBrush xmlns="clr-namespace:System.Windows.Media;assembly=PresentationCore" x:Key="GreenBrush">
            <SolidColorBrush.Color>
                <Color A="100">
                        <Color.G>219</Color.G>
                </Color>
            </SolidColorBrush.Color>
        </SolidColorBrush>
    </cmn:CustomDockPanel.Resources>

    <StackPanel>
        <StackPanel.Resources>
            <Style TargetType="{x:Type Button}">
                <Setter Property="Background" Value="{DynamicResource GreenBrush}" />
            </Style> 
        </StackPanel.Resources>

        <Button />
    
    </StackPanel>
    
</cmn:CustomDockPanel>
