<?xml version="1.0" encoding="utf-8"?>
<Root xmlns="http://tempuri.org/TypeProfile.xsd">
    <TypeName>Ellipse</TypeName>
    <Attributes>
		<!-- for Framework Element -->
        <Attribute>
            <PropertyName>Width</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>Length</PossibleValueType>
            </PossibleValueTypes>
        </Attribute>   
        <Attribute>
            <PropertyName>Height</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>Length</PossibleValueType>
            </PossibleValueTypes>
        </Attribute>      
        <Attribute>
            <PropertyName>Opacity</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>Double</PossibleValueType>
            </PossibleValueTypes>
        </Attribute>    
        <Attribute>
            <PropertyName>ID</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>ID</PossibleValueType>
            </PossibleValueTypes>
        </Attribute>           
        <Attribute>
            <PropertyName>Visibility</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>Visibility</PossibleValueType>
            </PossibleValueTypes>
        </Attribute>                  
        <Attribute>
            <PropertyName>Margin</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>Thickness</PossibleValueType>
            </PossibleValueTypes>
        </Attribute>    
            <!-- for Shape -->  
        <Attribute>
            <PropertyName>Fill</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>Color</PossibleValueType>
            </PossibleValueTypes>
        </Attribute> 
        <Attribute>
            <PropertyName>Stroke</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>Color</PossibleValueType>
            </PossibleValueTypes>
        </Attribute> 
        <Attribute>
            <PropertyName>StrokeThickness</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>Length</PossibleValueType>
            </PossibleValueTypes>
        </Attribute>    
        <Attribute>
            <PropertyName>StrokeStartLineCap</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>PenLineCap</PossibleValueType>
            </PossibleValueTypes>
        </Attribute>    
        <Attribute>
            <PropertyName>StrokeEndLineCap</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>PenLineCap</PossibleValueType>
            </PossibleValueTypes>
        </Attribute>  
        <Attribute>
            <PropertyName>StrokeDashCap</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>PenDashCap</PossibleValueType>
            </PossibleValueTypes>
        </Attribute>   
        <Attribute>
            <PropertyName>StrokeLineJoin</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>PenLineJoin</PossibleValueType>
            </PossibleValueTypes>
        </Attribute>  
        <Attribute>
            <PropertyName>StrokeMiterLimit</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>Double</PossibleValueType>
            </PossibleValueTypes>
        </Attribute> 
        <Attribute>
            <PropertyName>StrokeDashOffset</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>Double</PossibleValueType>
            </PossibleValueTypes>
        </Attribute>   
        <Attribute>
            <PropertyName>StrokeDashArray</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>DoubleCollection</PossibleValueType>
            </PossibleValueTypes>
        </Attribute>    
        <Attribute>
            <PropertyName>FillRule</PropertyName>
            <PossibleValueTypes>
                <PossibleValueType>FillRule</PossibleValueType>
            </PossibleValueTypes>
        </Attribute>         
         <!-- end for Shape -->                                            
    </Attributes>

    
   
    <ComplexProperties>
		<!-- shape -->
        <ComplexProperty>
            <PropertyName>Shape.Stroke</PropertyName>
            <PossibleTypes>
                <AllowedType>SolidColorBrush</AllowedType>             
            </PossibleTypes>
        </ComplexProperty>    
        <ComplexProperty>
            <PropertyName>Shape.Fill</PropertyName>
            <PossibleTypes>
                <AllowedType>SolidColorBrush</AllowedType>             
            </PossibleTypes>
        </ComplexProperty>     
        <!-- end  shape --> 
        
        <ComplexProperty>
            <PropertyName>Path.Data</PropertyName>
            <PossibleTypes>
                <AllowedType>EllipseGeometry</AllowedType>             
            </PossibleTypes>
        </ComplexProperty>         
    </ComplexProperties>    
</Root>