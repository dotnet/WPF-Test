<cmn:CustomStackPanel
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:cmn="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime"
      xmlns:sys="clr-namespace:System;assembly=mscorlib"
      Name="TestRoot"
>
  <cmn:CustomStackPanel.Resources>
  </cmn:CustomStackPanel.Resources>

  <Grid>
    <Grid.ColumnDefinitions>
      <ColumnDefinition Width="*" />
      <ColumnDefinition Width="*" />
    </Grid.ColumnDefinitions>

    
    <!-- 
    
    ****************************************
    **** Styled element section ************
    ****************************************
    -->
    <StackPanel Grid.Column="0" Name="RootForStyledElement" Width="100" Height="100" Background="Orange">
    </StackPanel>

    
    <!-- 
    
    ****************************************
    **** Unstyled element section **********
    ****************************************
    -->
    <StackPanel Grid.Column="1" Name="RootForUnstyledElement" Width="100" Height="100" Background="Pink">
    </StackPanel>
    
  </Grid>


</cmn:CustomStackPanel>
