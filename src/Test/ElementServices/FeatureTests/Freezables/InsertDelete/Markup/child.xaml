<Path StrokeThickness="15.0" PresentationOptions:Freeze="true"
 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
 xmlns:PresentationOptions="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options" 
 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
 mc:Ignorable="PresentationOptions"
>
  	<Path.Stroke >
              	<LinearGradientBrush  >
               		<LinearGradientBrush.GradientStops >
      	                 	<GradientStop Color="Blue" />
                        	<GradientStop Color="Yellow" />
                      		</LinearGradientBrush.GradientStops>
                  	</LinearGradientBrush>
        </Path.Stroke>
   	<Path.Data>
		     <EllipseGeometry  Center="60 300" RadiusX="50" RadiusY="75" PresentationOptions:Freeze="true" >
                   		<EllipseGeometry.Transform >
					<TranslateTransform PresentationOptions:Freeze="False" X="200" Y="230"  />
				</EllipseGeometry.Transform>
			</EllipseGeometry>

        </Path.Data>

	
    </Path>
