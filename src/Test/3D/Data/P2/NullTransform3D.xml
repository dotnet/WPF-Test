        Description="Transform3DGroup with null Children"
        TransformTarget="Model"
        TransformType="GroupNullChildren"

        Mesh="UnitPlaneTriangle"
        Material="255,255,0,0"
        Light="WhiteAmbient"
        />
</INIT>
