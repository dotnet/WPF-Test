    Camera="PerspectiveDefault"
    TransformType="Null"

    SaveExpectedZBuffer="false"
    SaveDiffFrameBuffer="false"
    SaveDiffZBuffer="false"
    >
    <VARIATION
        Description="Null transform on ModelVisual3D"
        TransformTarget="Visual3D"

        Mesh="Sphere 25 50 1.8"
        Material="255,255,0,0"
        Light="WhiteAmbient"
        />
    <VARIATION
        Description="Null transform on Model3DGroup"
        TransformTarget="Group"

        Mesh="Sphere 25 50 1.8"
        Material="255,255,0,0"
        Light="WhiteAmbient"
        />
    <VARIATION
        Description="Null transform on GeometryModel3D"
        TransformTarget="Model"

        Mesh="Sphere 25 50 1.8"
        Material="255,255,0,0"
        Light="WhiteAmbient"
        />
    <VARIATION
        Description="Null transform on AmbientLight"
        TransformTarget="Light"

        Mesh="Sphere 25 50 1.8"
        Material="255,255,0,0"
        Light="WhiteAmbient"
        />
    <VARIATION
        Description="Null transform on DirectionalLight"
        TransformTarget="Light"

        Mesh="Sphere 25 50 1.8"
        Material="255,255,0,0"
        Light="WhiteDirectionalNegZ"
