<Canvas
 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
>
<Button Name="button">Button</Button>
<CheckBox Name="checkbox" Canvas.Left="100">CheckBox</CheckBox>
<ComboBox Name="combobox" Canvas.Left="180">
<ComboBoxItem>Item1</ComboBoxItem>
<ComboBoxItem>Item2</ComboBoxItem>
<ComboBoxItem>Item3</ComboBoxItem>
</ComboBox>
<Expander Name="expander" Canvas.Left="350"/>
<Label Name="label" Canvas.Left="400">Label</Label>
<ListBox Name="listbox" Canvas.Left="450">
<ListBoxItem Name="listboxitem">item1</ListBoxItem>
<ListBoxItem>item1</ListBoxItem>
<ListBoxItem>item1</ListBoxItem>
</ListBox>
<ListView Name="listview" Canvas.Left="510" Height="50"/>
<Menu Name="menu" Canvas.Left="650">
	<MenuItem Header="Header 1">
		<MenuItem Header="Item 1">
		</MenuItem>
	</MenuItem>
	<MenuItem Header="Header 2" Name="menuitem">
	</MenuItem>
</Menu>
<ProgressBar Name="progressbar" Canvas.Top="80"/>
<RadioButton Name="radiobutton" Canvas.Top="80" Canvas.Left="210">RadioButton</RadioButton>
<RepeatButton Name="repeatbutton" Canvas.Top="80" Canvas.Left="300">RepeatButton</RepeatButton>
<ScrollBar Name="scrollbar" Canvas.Top="80" Canvas.Left="400" Width="100" Orientation="Horizontal"/>
<ScrollViewer Name="scrollviewer" Width="50" Height="50" Canvas.Top="160" Canvas.Left="500"/>
<Separator Name="separator" Width="50" Height="50" Canvas.Top="160" Canvas.Left="580"/>
<Slider Name="slider" Width="50" Height="50" Canvas.Top="160" Canvas.Left="650"/>
<StatusBarItem Name="statusbaritem" Width="100" Height="50" Canvas.Top="160" Canvas.Left="710">StatusBarItem</StatusBarItem>
<StatusBar Name="statusbar" Canvas.Top="80" Canvas.Left="510" Width="100" Height="20"/>
<TabControl Name="tabcontrol" Canvas.Top="80" Canvas.Left="620">
	<TabItem Name="tabitem" Header="Item1">
		<StackPanel><Button>Button1</Button></StackPanel>
	</TabItem>
	<TabItem Header="Item2" IsEnabled="false">
		<StackPanel><Button>Button 2</Button></StackPanel>
	</TabItem>
	<TabItem Header="Item3">
		<StackPanel><Button>Button 3</Button></StackPanel>
	</TabItem>
</TabControl>
<Thumb Name="thumb" Canvas.Top="160"/>
<ToggleButton Name="togglebutton" Canvas.Top="160" Canvas.Left="20">ToggleButton</ToggleButton>
<ToolBarTray Canvas.Top="160" Canvas.Left="100">
	<ToolBar Name="toolbar" Header="Abc">
	<TextBox  />
	</ToolBar>
	<ToolBar Header="Zzz">
	</ToolBar>
</ToolBarTray>
<TreeView Name="treeview" Canvas.Top="160" Canvas.Left="220" Grid.Row="2" Grid.Column="1" Margin="5">
    <TreeViewItem Name="treeviewitem" Header="My Computer">
	<TreeViewItem Header="Floppy Drive" />
	<TreeViewItem Header="Hard Drive">
	    <TreeViewItem Header="boot.ini" />
	    <TreeViewItem Header="config.sys" />
	</TreeViewItem>
    </TreeViewItem>
    <TreeViewItem Header="My Network Places">
	<TreeViewItem Header="msn.com" />
	<TreeViewItem Header="msweb" />
	<TreeViewItem Header="Other sites">
	    <TreeViewItem Header="World News" />
	    <TreeViewItem Header="Local News" />
	    <TreeViewItem Header="Business News" />
	</TreeViewItem>
    </TreeViewItem>
</TreeView>

<Image Name="image" Canvas.Top="220" Source="cutepig.jpg" />

</Canvas>
